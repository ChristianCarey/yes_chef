<h4>Your most popular menu items</h4>
<table class="table table-striped table-bordered">
  <tr>
    <th>
      Item Name
    </th>
    <th>
      Total Quantity Sold
    </th>
  </tr>
  <% menu_items.each do |menu_item| %>
    <tr>
      <td>
        <%= link_to menu_item.name, menu_item %>
      </td>
      <td>
        <%= menu_item.order_items_count %>
      </td>
    </tr>
  <% end %>
</table>