<h4>Your Top Customers - <%= link_to "View All", chef_customers_path %></h4>
<table class="table table-striped table-bordered">
<tr>
  <th>
    Name
  </th>
  <th>
    Total Orders Placed
  </th>
</tr>
  <% customers.each do |customer| %>
  <tr>
    <td>
      <%= link_to customer.name, user_profile_path(customer) %>
    </td>
    <td>
      <%= customer.placed_orders_count %>
    </td>
  </tr>
  <% end %>
</table>
